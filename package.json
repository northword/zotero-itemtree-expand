{
  "name": "zotero-itemtree-expand",
  "type": "module",
  "version": "3.0.4",
  "packageManager": "pnpm@10.4.1",
  "description": "Expand the item tree for Zotero",
  "config": {
    "addonName": "Expand Item Tree",
    "addonID": "itemtree-expand@northword.cn",
    "addonRef": "itemtree-expand",
    "addonInstance": "ItemTreeExpand",
    "prefsPrefix": "extensions.itemtreeexpand"
  },
  "author": "northword",
  "license": "AGPL-3.0-or-later",
  "homepage": "https://github.com/northword/zotero-itemtree-expand#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northword/zotero-itemtree-expand.git"
  },
  "bugs": {
    "url": "https://github.com/northword/zotero-itemtree-expand/issues"
  },
  "scripts": {
    "start": "zotero-plugin serve",
    "build": "zotero-plugin build && tsc --noEmit",
    "lint:check": "prettier --check . && eslint .",
    "lint:fix": "prettier --write . && eslint . --fix",
    "release": "zotero-plugin release",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "zotero-plugin-toolkit": "^4.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3",
    "zotero-plugin-scaffold": "^0.2.3",
    "zotero-types": "^4.0.0"
  }
}
